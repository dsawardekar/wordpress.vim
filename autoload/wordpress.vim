function! s:CtrlPExtension_do_accept(mode, str) dict
  if self.inited && !exists('g:speckle_mode')
    call ctrlp#exit()
    let self.inited = 0
  endif
  let result = self.agent.accept(a:mode, a:str)
  return result
endfunction

