  let tags_option = self.lookup('tags_option')
  if ctags_builder.has_tags()
    let tags_path = fnamemodify(ctags_builder.get_project_tags(), ':p')
    if !(tags_option.get_tags() =~# tags_path)
      call tags_option.prepend(tags_path)
