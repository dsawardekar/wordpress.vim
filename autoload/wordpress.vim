  return self.current_buffer_project_path() . "/readme.txt"
endfunction

function! s:ValidateReadmeCommand_display_list(label, list) dict
  let i = 1
  for item in a:list
    let msg = a:label . " " . '#' . i . ": " . item
    call s:echo_msg(msg)
    let i += 1
  endfor
endfunction

