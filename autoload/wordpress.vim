    return
  endif
  let tags_path = ctags_builder.get_project_tags()
  if filereadable(tags_path)
    call delete(tags_path)
  endif
  let didGenerate = self.generate_ctags('Regenerating')
  if didGenerate
    call s:echo_msg('WordPress: Regenerating ctags DONE')
  endif
